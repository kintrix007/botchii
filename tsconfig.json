{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "lib": [ "ES2019", "ES2019.Object", "ES2019.Array" ],
        "allowJs": false,
        "outDir": "build",
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strictNullChecks": true,
        "removeComments": true
    }
}
